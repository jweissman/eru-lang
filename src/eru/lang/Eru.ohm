Arithmetic {
  Exp
    = AddExp

  AddExp
    = AddExp "+" MulExp  -- plus
    | AddExp "-" MulExp  -- minus
    | MulExp

  MulExp
    = MulExp "*" LogExp  -- mult
    | MulExp "/" LogExp  -- div
    | LogExp

  LogExp
    = LogExp "||" PriExp  -- or
    | LogExp "&&" PriExp  -- and
    | "!" LogExp -- not
    | PriExp

  PriExp
    = "(" Exp ")"  -- paren
    | bool
    | number

  number
    = digit+
    // | ident

  bool
    = "true" -- true
    | "false" -- false
}
